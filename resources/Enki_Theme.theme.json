{
  "name": "Enki Aster Theme",
  "dark": true,
  "author": "Cyberquarks",
  "editorScheme": "/Enki_Theme.xml",
  "colors": {
    "accent": "#1E1E25",
    "border": "#1E1E25"
  },
  "ui": {
    "*": {
      "background": "#16161B",
      "foreground": "#5B5D6D",
      "selectionBackground": "#20222C",
      "selectionForeground": "#9395B3",
      "borderColor":"#1E1E25",
      "disabledBorderColor": "#1E1E25",
      "focusColor": "#1E1E25",
      "focusedBorderColor": "#1E1E25",
      "hoverBorderColor": "#1E1E25",
      "pressedBorderColor": "#1E1E25",
      "separatorColor": "#1E1E25",
      "lineSeparatorColor": "#1E1E25"
