      "focusColor": "#484A58",
      "focusedBorderColor": "#484A58",
      "borderColor": "#1E1E25"
    },
    "Button": {
      "default": {
        "startBackground": "#404F76",
        "endBackground": "#404F76",
        "focusedBorderColor": "#484A58",
        "focusColor": "#1E1E25",
        "foreground": "#1E1E25"
      },
      "startBackground": "#1E1E25",
      "endBackground": "#1E1E25",
      "focusedBorderColor": "#1E1E25",
      "disabledBorderColor": "#1E1E25",
      "borderColor": "#1E1E25"
